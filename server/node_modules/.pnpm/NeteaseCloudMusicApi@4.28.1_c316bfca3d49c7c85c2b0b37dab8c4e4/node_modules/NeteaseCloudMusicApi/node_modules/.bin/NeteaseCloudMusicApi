#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/houyu/Desktop/my_project/echo-music/server/node_modules/.pnpm/NeteaseCloudMusicApi@4.28.1_c316bfca3d49c7c85c2b0b37dab8c4e4/node_modules/NeteaseCloudMusicApi/node_modules:/Users/houyu/Desktop/my_project/echo-music/server/node_modules/.pnpm/NeteaseCloudMusicApi@4.28.1_c316bfca3d49c7c85c2b0b37dab8c4e4/node_modules:/Users/houyu/Desktop/my_project/echo-music/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/houyu/Desktop/my_project/echo-music/server/node_modules/.pnpm/NeteaseCloudMusicApi@4.28.1_c316bfca3d49c7c85c2b0b37dab8c4e4/node_modules/NeteaseCloudMusicApi/node_modules:/Users/houyu/Desktop/my_project/echo-music/server/node_modules/.pnpm/NeteaseCloudMusicApi@4.28.1_c316bfca3d49c7c85c2b0b37dab8c4e4/node_modules:/Users/houyu/Desktop/my_project/echo-music/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../app.js" "$@"
else
  exec node  "$basedir/../../app.js" "$@"
fi
