#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/houyu/Desktop/my_project/echo-music/server/node_modules/mkdirp/bin/node_modules:/Users/houyu/Desktop/my_project/echo-music/server/node_modules/mkdirp/node_modules:/Users/houyu/Desktop/my_project/echo-music/server/node_modules:/Users/houyu/Desktop/my_project/echo-music/node_modules:/Users/houyu/Desktop/my_project/node_modules:/Users/houyu/Desktop/node_modules:/Users/houyu/node_modules:/Users/node_modules:/node_modules:/Users/houyu/Desktop/my_project/echo-music/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/houyu/Desktop/my_project/echo-music/server/node_modules/mkdirp/bin/node_modules:/Users/houyu/Desktop/my_project/echo-music/server/node_modules/mkdirp/node_modules:/Users/houyu/Desktop/my_project/echo-music/server/node_modules:/Users/houyu/Desktop/my_project/echo-music/node_modules:/Users/houyu/Desktop/my_project/node_modules:/Users/houyu/Desktop/node_modules:/Users/houyu/node_modules:/Users/node_modules:/node_modules:/Users/houyu/Desktop/my_project/echo-music/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mkdirp/bin/cmd.js" "$@"
else
  exec node  "$basedir/../mkdirp/bin/cmd.js" "$@"
fi
